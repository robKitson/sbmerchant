
<div class="col-sm-10 col-md-6">
  <%= form_for @owner info, as: :owner info, url: (@owner info.new_record? ? owner_infos_path(@owner info) : owner_info_path(@owner info)), :html => {class:"form-horizontal"}  do |f| %>
    <% if @owner info.errors.any? %>
      <div class="alert alert-danger" role="alert">
        <div><strong><%= pluralize(@owner info.errors.count, "error") %></strong> prohibited this owner info from being saved:
        </div>

        <ul>
        <% @owner info.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
        </ul>
      </div>
    <% end %>

      <div class="form-group">
